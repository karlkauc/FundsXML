									<xs:documentation xml:lang="en">The distributing value</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="nichtausschuettend">
					<xs:annotation>
						<xs:documentation xml:lang="de">Ein thesaurierender Fond</xs:documentation>
						<xs:documentation xml:lang="en">A fund which is not distributing</xs:documentation>
					</xs:annotation>
