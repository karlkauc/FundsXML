				</xs:annotation>
			</xs:element>
			<xs:element name="ChangeInValue" type="PercentageType">
				<xs:annotation>
					<xs:documentation>Wertaenderung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ForeignIncomeTax" type="PercentageType">
