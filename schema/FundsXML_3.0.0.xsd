						<xs:sequence>
							<xs:element name="CapitalYieldsTax" type="xs:decimal">
								<xs:annotation>
									<xs:documentation xml:lang="de">Kapitalertragssteuer</xs:documentation>
									<xs:documentation xml:lang="en">Capital Yields Tax</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="SolidaritaetszuschlagKEST" type="xs:decimal">
								<xs:annotation>
									<xs:documentation xml:lang="de">Solidaritaetszuschlag der Kapitalertragssteuer</xs:documentation>
									<xs:documentation xml:lang="en">Capital Yields Tax - Solidarity Surcharge</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ZAST" type="xs:decimal">
								<xs:annotation>
									<xs:documentation xml:lang="de">Zinsabschlagssteuer</xs:documentation>
									<xs:documentation xml:lang="en">Withholding Tax on Interest</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="SolidaritaetszuschlagZAST" type="xs:decimal">
								<xs:annotation>
									<xs:documentation xml:lang="de">Solidaritaetszuschlag der Zinsabschlagssteuer.</xs:documentation>
									<xs:documentation xml:lang="en">Withholding Tax on Interest - Solidarity Surcharge</xs:documentation>
								</xs:annotation>
